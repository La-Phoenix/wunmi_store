{
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://wunmi-commerce-server.vercel.app/api/v1/:path*" },
    {
      "source": "/auth/:path*",
      "destination": "https://wunmi-commerce-server.vercel.app/api/v1/auth/:path*"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
